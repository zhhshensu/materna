{
  "name": "materna",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently  \"npm run dev:app\" \"npm run dev:electron\"",
    "build": "rimraf ./build/ && concurrently  \"npm run build:app\" \"npm run build:electron\"",
    "release": "",
    "start:app": "npm run dev:app",
    "dev:app": "vite --config ./vite.config.ts",
    "preview:app": "vite preview --config ./vite.config.ts",
    "build:app": "tsc -p src/app/tsconfig.json && cross-env NODE_ENV=production vite build --config ./vite.config.ts",
    "lint": "eslint app --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fix": "eslint app --fix",
    "lint:style": "stylelint app/**/*.{tsx,jsx,css} --cache --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md,tsx,jsx}\"",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "nest:start": "nest start --watch -p src/electron/tsconfig.json -c src/electron/nest-cli.json",
    "start:electron": "npm run dev:electron",
    "dev:electron": "cross-env NODE_ENV=development tsup --config tsup.config.ts --watch --onSuccess 'npx electron ./dist/electron/main.js'",
    "dev:electron:run": "electron ./dist/electron/main.js",
    "build:electron": "nest build -p src/electron/tsconfig.build.json -c src/electron/nest-cli.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.5",
    "@nestjs/core": "^10.2.5",
    "@nestjs/platform-express": "^10.2.5",
    "axios": "^1.5.1",
    "electron": "^26.2.1",
    "formik": "^2.4.5",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^9.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-transition-group": "^4.4.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@swc/core": "^1.3.86",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.6.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "glob": "^10.3.4",
    "husky": "^8.0.0",
    "only-allow": "^1.1.1",
    "postcss": "^8.4.29",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "source-map-support": "^0.5.21",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-static-copy": "^0.17.0"
  }
}
